{
    "name": "advancedynamic/codeigniter-attributeroutes",
    "description": "CodeIgniter4 Attribute Routes module",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Advancedynamic\\Codeigniter\\Attributeroutes\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Advance dynamic Software",
            "email": "advancedynamicsoftware@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "codeigniter4/devkit": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true 
}
